
{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/app.js"
      }
    ],
    "env": {
      "PORT": "5000",
      "MONGO_URI": "MONGODB_CONNECTION_STRING",
      "JWT_SECRET": "YOUR_JWT_SECRET"
    }
  }